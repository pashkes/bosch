mixin in-complect()
  section.in-complect.noise-gradient
    .container
      //button.btn.js-second-complect(type="button" data-slide="2") go 2 slide
      h2.in-complect__title Что входит в комлект Bosch GO
      ul.in-complect__main-slider.js-in-complect-main-slider
        li.in-complect__main-slide
          .row
            .col-md-6.col-xl-8
              ul.in-complect__photos.js-in-complect-photos
                li
                  img(src="img/01_box.png")
                li
                  img(src="img/02_box.png")
            .col-md-6.col-xl-4
              ul.in-complect__has
